#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/eray/projects/OCPP/mobile/node_modules/.pnpm/expo@54.0.33_@babel+core@7.29.0_@expo+metro-runtime@6.1.2_react-native@0.81.5_@babel+co_7ac90c6562476295b5009ad655ea6711/node_modules/expo/bin/node_modules:/home/eray/projects/OCPP/mobile/node_modules/.pnpm/expo@54.0.33_@babel+core@7.29.0_@expo+metro-runtime@6.1.2_react-native@0.81.5_@babel+co_7ac90c6562476295b5009ad655ea6711/node_modules/expo/node_modules:/home/eray/projects/OCPP/mobile/node_modules/.pnpm/expo@54.0.33_@babel+core@7.29.0_@expo+metro-runtime@6.1.2_react-native@0.81.5_@babel+co_7ac90c6562476295b5009ad655ea6711/node_modules:/home/eray/projects/OCPP/mobile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/eray/projects/OCPP/mobile/node_modules/.pnpm/expo@54.0.33_@babel+core@7.29.0_@expo+metro-runtime@6.1.2_react-native@0.81.5_@babel+co_7ac90c6562476295b5009ad655ea6711/node_modules/expo/bin/node_modules:/home/eray/projects/OCPP/mobile/node_modules/.pnpm/expo@54.0.33_@babel+core@7.29.0_@expo+metro-runtime@6.1.2_react-native@0.81.5_@babel+co_7ac90c6562476295b5009ad655ea6711/node_modules/expo/node_modules:/home/eray/projects/OCPP/mobile/node_modules/.pnpm/expo@54.0.33_@babel+core@7.29.0_@expo+metro-runtime@6.1.2_react-native@0.81.5_@babel+co_7ac90c6562476295b5009ad655ea6711/node_modules:/home/eray/projects/OCPP/mobile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../expo/bin/autolinking" "$@"
else
  exec node  "$basedir/../expo/bin/autolinking" "$@"
fi
